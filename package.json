{
  "name": "sci-lisp",
  "displayName": "Sci-Lisp",
  "description": "Sci-Lisp language support",
  "version": "0.0.2",
  "publisher": "chaploud",
  "icon": "images/scilisp.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/chaploud/Sci-Lisp-vscode-extension"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "lisp",
      "aliases": ["Sci-Lisp", "lisp"],
      "extensions": [".lisp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lisp",
      "scopeName": "source.lisp",
      "path": "./syntaxes/lisp.tmLanguage.json"
    }]
  }
}
